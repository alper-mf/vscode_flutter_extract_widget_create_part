{
  "name": "flutter-extract-widget-create-part",
  "displayName": "Flutter Extract Widget and Create Part",
  "description": "Extracts a Flutter widget and creates a part file to use it.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.extractFlutterWidgetCreatePart",
        "title": "Extract Flutter Widget and Create Part"
      }
    ],
    "languages": [
      {
        "id": "dart",
        "extensions": [".dart"],
        "aliases": ["Dart"],
        "filenames": [],
        "configuration": "./language-configuration.json"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.extractFlutterWidgetCreatePart",
          "group": "navigation",
          "when": "editorLangId == 'dart'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
